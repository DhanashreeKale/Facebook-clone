{
  "name": "nestjs-seed",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "npm run lint && npm run prebuild && nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "set NODE_ENV=dev&& npm run lint:fix && nest start --watch",
    "start:local": "set NODE_ENV=dev&& npm run lint && nest start --watch",
    "start:docker": "export NODE_ENV=docker&& npm run lint && nest start --watch",
    "start:hmr": "set NODE_ENV=local&& npm run lint && nest build --webpack --webpackPath webpack-hmr.config.js --watch",
    "start:debug": "npm run lint && nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js -f ./ormconfig.json",
    "typeorm:seed": "node --require ts-node/register ./node_modules/typeorm/cli.js -f ./ormconfig-seed.json",
    "sonar": "node sonar-project.js"
  },
  "dependencies": {
    "@nestjs/common": "^7.6.15",
    "@nestjs/config": "^0.6.3",
    "@nestjs/core": "^7.4.4",
    "@nestjs/jwt": "^7.1.0",
    "@nestjs/passport": "^7.1.0",
    "@nestjs/platform-express": "^7.6.18",
    "@nestjs/serve-static": "^2.1.3",
    "@nestjs/swagger": "^4.6.1",
    "@nestjs/typeorm": "^7.1.4",
    "@types/app-root-path": "^1.2.4",
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.7.0",
    "@types/config": "^0.0.41",
    "@types/csurf": "^1.9.36",
    "@types/express-session": "^1.17.0",
    "@types/morgan": "^1.9.1",
    "app-root-path": "^3.0.0",
    "bcrypt": "^5.0.0",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "compression": "^1.7.4",
    "config": "^3.3.7",
    "cookie-parser": "^1.4.5",
    "csurf": "^1.11.0",
    "express-basic-auth": "^1.2.0",
    "express-session": "^1.17.1",
    "helmet": "^4.1.1",
    "jwt-decode": "^3.1.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.4.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rotating-file-stream": "^2.1.3",
    "rxjs": "^6.6.3",
    "source-map-support": "^0.5.19",
    "swagger-ui-express": "^4.1.4",
    "typeorm": "^0.2.28",
    "uuid": "^8.3.2",
    "validate-date": "^2.0.0",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^4.5.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.5.1",
    "@nestjs/schematics": "^7.1.2",
    "@nestjs/testing": "^7.4.4",
    "@types/express": "^4.17.8",
    "@types/jest": "^26.0.14",
    "@types/node": "^15.0.2",
    "@types/passport-jwt": "^3.0.3",
    "@types/source-map-support": "^0.5.3",
    "@types/supertest": "^2.0.10",
    "@types/webpack-env": "^1.15.3",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^4.29.1",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^26.5.0",
    "prettier": "^2.3.2",
    "run-script-webpack-plugin": "^0.0.11",
    "sonarqube-scanner": "^2.8.0",
    "start-server-webpack-plugin": "^2.2.5",
    "supertest": "^6.1.3",
    "ts-jest": "^26.4.1",
    "ts-loader": "^9.1.2",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "4.0.3",
    "webpack": "^5.28.0",
    "webpack-node-externals": "^3.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
